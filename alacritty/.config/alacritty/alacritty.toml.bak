[font]
size = 10

[font.normal]
family = "Dejavu Sans Mono"
style = "Regular"

[window]
opacity = 0.95

[window.padding]
x = 5
y = 5

[selection]
save_to_clipboard = true

# Colors (Doom One)

# Default colors
[colors.primary]
background = '#282c34'
foreground = '#bbc2cf'

# Normal colors
[colors.normal]
black   = '#282c34'
red     = '#ff6c6b'
green   = '#98be65'
yellow  = '#ecbe7b'
blue    = '#51afef'
magenta = '#c678dd'
cyan    = '#46d9ff'
white   = '#bbc2cf'
